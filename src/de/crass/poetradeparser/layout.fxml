<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.crass.poetradeparser.Main">
   <center>
      <TabPane prefHeight="480.0" prefWidth="640.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="CurrencyOverview">
               <content>
                  <AnchorPane>
                     <children>
                        <Label layoutX="194.0" layoutY="208.0" text="Press Update to Update">
                           <font>
                              <Font size="24.0" />
                           </font>
                        </Label>
                        <ListView fx:id="currencyList" styleClass="deal-list" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                     </children>
                  </AnchorPane>
               </content>
          </Tab>
          <Tab text="PlayerOverview">
               <content>
                  <ListView fx:id="playerDealList" styleClass="deal-list" />
               </content>
          </Tab>
            <Tab text="Offers">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <top>
                        <Text fill="#d8d8d8" strokeType="OUTSIDE" strokeWidth="0.0" text="Not implemented" BorderPane.alignment="CENTER">
                           <font>
                              <Font size="39.0" />
                           </font>
                           <BorderPane.margin>
                              <Insets top="100.0" />
                           </BorderPane.margin>
                        </Text>
                     </top>
                     <bottom>
                        <Text fill="#d8d8d8" strokeType="OUTSIDE" strokeWidth="0.0" text="¯\_(ツ)_/¯" BorderPane.alignment="CENTER">
                           <font>
                              <Font size="94.0" />
                           </font>
                           <BorderPane.margin>
                              <Insets bottom="100.0" />
                           </BorderPane.margin>
                        </Text>
                     </bottom>
                  </BorderPane>
               </content>
            </Tab>
            <Tab text="Settings">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <CheckBox fx:id="filterWithoutAPI" layoutX="37.0" layoutY="52.0" mnemonicParsing="false" selected="true" text="Filter Offers without API" />
                        <Label layoutX="355.0" layoutY="25.0" text="Player Settings" />
                        <CheckBox fx:id="filterInvalid" layoutX="37.0" layoutY="82.0" mnemonicParsing="false" selected="true" text="Filter Offers with insufficient stock" />
                        <ComboBox fx:id="currencyFilterCB" layoutX="37.0" layoutY="114.0" prefHeight="37.0" prefWidth="132.0" />
                        <ListView fx:id="currencyFilterList" layoutX="37.0" layoutY="157.0" prefHeight="210.0" prefWidth="200.0" />
                        <Button fx:id="removeCurrencyFilterBtn" layoutX="37.0" layoutY="384.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="201.0" text="Remove" />
                        <Button fx:id="addPlayerButton" layoutX="493.0" layoutY="61.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="62.0" text="+" />
                        <ListView fx:id="playerListView" layoutX="355.0" layoutY="113.0" prefHeight="127.0" prefWidth="200.0" />
                        <TextField fx:id="playerField" layoutX="355.0" layoutY="61.0" prefHeight="36.0" prefWidth="131.0" />
                        <Button fx:id="addCurrencyFilterBtn" layoutX="178.0" layoutY="114.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="59.0" text="+" />
                        <Label layoutX="355.0" layoutY="308.0" text="League" />
                        <Button fx:id="removePlayerBtn" layoutX="355.0" layoutY="262.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="201.0" text="Remove" />
                        <TextField fx:id="leagueField" layoutX="355.0" layoutY="335.0" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </center>
   <bottom>
      <HBox prefHeight="40.0" prefWidth="640.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="updateButton" fx:id="updateButton" defaultButton="true" minWidth="100.0" mnemonicParsing="false" prefHeight="73.0" prefWidth="70.0" text="Update">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </Button>
            <TextArea fx:id="console" editable="false" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" prefHeight="0.0" promptText="Status console" wrapText="true" HBox.hgrow="ALWAYS" />
            <Label fx:id="version" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" minWidth="50.0" text="Version" textAlignment="CENTER" textOverrun="CLIP">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" top="5.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
   <top>
      <ComboBox fx:id="primaryComboBox" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
